{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": ["react-app", "prettier"],
  "parserOptions": {
    "ecmaVersion": 11
  },
  "plugins": [
    "react", "prettier"
  ],
  "rules": {
    "semi": "error",
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "jsxSingleQuote": true,
        "singleQuote": true,
        "useTabs": true
      }
    ]
  }
}
